<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <a class="navbar-brand" href="/">EJS Is Fun</a>
  <ul class="nav nav-pills">
    <% for (let i = 0; i < nav.length; i++) { %>
      <% if (nav[i].title === title) { %>
        <li class="nav-item">
          <a class="nav-link active" href="<%= `${nav[i].url}` %>"><%= `${nav[i].title}` %></a>
        </li>
        <% } else if (nav[i].title === "EAFC Mouscron") { %>
          <li class="nav-item">
            <a class="nav-link" href="<%= `${nav[i].url}` %>" target="_blank"><%= `${nav[i].title}` %></a>
          </li>
      <% } else { %>
        <li class="nav-item">
          <a class="nav-link" href="<%= `${nav[i].url}` %>"><%= `${nav[i].title}` %></a>
        </li>
      <% } %>
    <% } %>
  </ul>
</nav>